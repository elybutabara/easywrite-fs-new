{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "anhskohbo/no-captcha": "^3.7",
        "barryvdh/laravel-dompdf": "^3.1",
        "barryvdh/laravel-ide-helper": "*",
        "barryvdh/laravel-translation-manager": "^0.6",
        "firebase/php-jwt": "^6.4",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "2.7",
        "laravel/framework": "^12.18",
        "laravel/helpers": "^1.7",
        "laravel/socialite": "^5.17",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/fractal": "^0.17.0",
        "league/omnipay": "^3.2",
        "maatwebsite/excel": "~3.1.38",
        "omnipay/paypal": "^3.0",
        "phpoffice/phpword": "^0.18",
        "spatie/flysystem-dropbox": "*",
        "sveaekonomi/webpay": "^3.15",
        "symfony/event-dispatcher": "^7.2",
        "symfony/polyfill-iconv": "^1.8",
        "teodortalov/citrix": "^1.2",
        "unisharp/laravel-filemanager": "^2.10",
        "van-ons/laraberg": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.5.3",
        "filp/whoops": "^2.8",
        "fakerphp/faker": "^1.23",
        "nunomaduro/collision": "^8.6",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan ide-helper:generate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
